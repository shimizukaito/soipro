generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model program_logs {
  id         Int       @id @default(autoincrement())
  user_id    String?
  language   String?
  code       String?
  created_at DateTime? @default(now()) @db.Timestamp(6)
}

model ButtonLog {
  id        Int      @id @default(autoincrement())
  type      String
  timestamp DateTime
}

model Post {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  content    String
  theme      Int
  user       String
  output     String
  order      Int
  isLatest   Boolean  @default(true)
}
